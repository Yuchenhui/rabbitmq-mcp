{
  "name": "rabbitmq-mcp-adv",
  "version": "0.2.2",
  "description": "RabbitMQ MCP Server forked from https://github.com/kmitchell/rabbitmq-mcp",
  "type": "module",
  "author": "github.com/yuchenhui",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "jest": "^29.0.0",
    "shx": "^0.4.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "rabbitmq-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:quick": "node quick-test.js",
    "test:integration": "node test-integration.js",
    "test:manual": "echo '请查看 MANUAL_TEST_GUIDE.md 进行手动测试'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchenhui/rabbitmq-mcp"
  },
  "homepage": "https://github.com/yuchenhui/rabbitmq-mcp",
  "bugs": {
    "url": "https://github.com/yuchenhui/rabbitmq-mcp/issues"
  },
  "keywords": [
    "amqp",
    "cloudamqp",
    "rabbitmq",
    "mcp",
    "modelcontextprotocol",
    "rabbitmq-mcp"
  ],
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./jest.setup.mjs"
    ],
    "testMatch": [
      "**/src/tests/**/*.test.mjs"
    ]
  }
}